{
  "name": "algorithm-practice-extension",
  "version": "1.0.2",
  "description": "chrome extension to practice algorithms",
  "scripts": {
    "start": "echo 'ğŸš€ Starting the app...' && npm run build && node dist/index.js",
    "build": "echo 'ğŸ”¨ Building the app...' && webpack --mode production",
    "test": "echo 'ğŸ§ª Running tests...' && jest --config jest.config.cjs --coverage",
    "format": "echo 'ğŸ–‹ï¸ Formatting code...' && prettier --write .",
    "lint": "echo 'ğŸ§¹ Linting code...' && eslint './**/*.{js,ts}' --ignore-path eslintignore",
    "precommit": "echo 'ğŸ”’ Precommit: Linting and testing code...' && npm run lint && npm test",
    "pretest": "echo 'ğŸ” Pretest: Linting code...' && npm run lint",
    "watch": "echo 'ğŸ‘€ Watching files...' && jest --watch",
    "debug": "echo 'ğŸ Starting the app in debug mode...' && NODE_ENV=development nodemon --inspect src/index.js",
    "clean": "echo 'ğŸ§½ Cleaning up...' && rm -rf ./node_modules ./dist && rm package-lock.json",
    "prepare": "husky install # ğŸ¶ Setting up Husky",
    "remove-dsstore": "echo 'ğŸ—‘ï¸ Removing .DS_Store files...' && find . -name '.DS_Store' -delete",
    "create-zip": "echo 'ğŸ“¦ Creating ZIP file...' && cd .. && zip -r algorithm-practice-extension.zip algorithm-practice-extension/*",
    "package-extension": "echo 'ğŸ“¦ Packaging extension...' && npm run remove-dsstore && npm run create-zip"
  },
