{
  "manifest_version": 3,
  "name": "Algorithm Practice Extension",
  "description": "A Chrome extension to practice algorithms",
  "version": "1.0.0",
  "action": {
      "default_popup": "public/popup.html",
      "default_icon": {
          "16": "icons/icon16.png",
          "32": "icons/icon32.png",
          "128": "icons/icon128.png"
      }
  },
  "background": {
      "service_worker": "dist/background.js"
  },
  "permissions": [
      "storage",
      "activeTab"
  ],
  "content_scripts": [
      {
          "matches": [
              "<all_urls>"
          ],
          "js": [
              "dist/content.js"
          ]
      }
  ]
}